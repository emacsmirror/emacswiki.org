        do (loop for 汉字 across 汉字str
                 do (let ((existing (gethash 汉字 ayr-china-util-拼音汉字-table)))
                      (puthash 汉字
                               (if (and existing
                                        (not (string-equal existing 拼音)))
                                   (concat 拼音 "|" existing)
                                 拼音)
                               ayr-china-util-拼音汉字-table)))))

(defun ayr-pinyin-of-chinese (string)
  (apply #'concat
         (loop for c across string
               collecting (concat " " (or (gethash c ayr-china-util-拼音汉字-table)
                                          c)))))

(defun ayr-pinyin-of-chinese-at-point ()
  (interactive)
  (let ((c (char-after (point))))
    (delete-char 1 t)
    (insert (or (gethash c ayr-china-util-拼音汉字-table)
                c))))

(defun ayr-pinyin-of-chinese-in-region (begin end)
  (interactive "r")
  (let ((string (buffer-substring-no-properties begin end)))
    (save-excursion
      (goto-char begin)
      (kill-region begin end)
      (insert (ayr-pinyin-of-chinese string)))))
